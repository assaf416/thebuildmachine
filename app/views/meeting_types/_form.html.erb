<%= simple_form_for(@meeting_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id %>
    <%= f.input :name %>
    <%= f.input :cost %>
    <%= f.input :defult_time %>
    <%= f.input :phone_call %>
    <%= f.input :video_conf %>
    <%= f.input :meeting %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
