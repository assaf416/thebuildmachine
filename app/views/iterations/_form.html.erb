<%= simple_form_for(@iteration) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :project_id %>
    <%= f.input :name %>
    <%= f.input :starts_on %>
    <%= f.input :ends_on %>
    <%= f.input :current %>
    <%= f.input :developers %>
    <%= f.input :testers %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
