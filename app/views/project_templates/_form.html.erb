<%= simple_form_for(@project_template) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :manual %>
    <%= f.input :git_url %>
    <%= f.input :application %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
