<%= simple_form_for(@recipe) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :user_id %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :body %>
    <%= f.input :image_url %>
    <%= f.input :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
