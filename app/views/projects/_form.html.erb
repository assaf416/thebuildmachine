<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :created_by_user_id %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :icon %>
    <%= f.input :project_template_id %>
    <%= f.input :production_server_url %>
    <%= f.input :test_server_url %>
    <%= f.input :stageing_server_url %>
    <%= f.input :build_machine_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
